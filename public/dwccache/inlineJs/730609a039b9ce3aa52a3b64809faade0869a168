s:490:"<script type="text/javascript">$(function(){var url=window.location.pathname,urlRegExp=new RegExp(url.replace(/\/$/,'')+"$");$('ul li a').each(function(){if(urlRegExp.test(this.href.replace(/\/$/,''))){var href=$(this).parents().eq(0).attr('id');$(this).addClass('side-menu--active');$('#'+href).addClass('side-menu--active');Checkhref(href);}});function Checkhref(href){$('ul li a').each(function(){if($(this).attr('href')=="#"+href){$(this).addClass('side-menu--active');}});}});</script>";