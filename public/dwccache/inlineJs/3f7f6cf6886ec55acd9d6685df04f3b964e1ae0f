s:430:"<script type="text/javascript">$("#save").on("click",function(){$('#save').html('Đang xử lý...').prop('disabled',true);$.ajax({url:"https://api.sieuthicode.net/assets/ajaxs/Auth.php",method:"POST",data:{type:'UpdatePass',password:$("#password").val(),repassword:$("#repassword").val()},success:function(response){$("#thongbao").html(response);$('#save').html('Thay đổi mật khẩu').prop('disabled',false);}});});</script>";