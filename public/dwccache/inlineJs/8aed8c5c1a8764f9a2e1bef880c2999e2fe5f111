s:1003:"<script type="text/javascript">var Noti=function(type,msg){toastr.options={closeButton:true,progressBar:true,showMethod:'slideDown',timeOut:1500};if(type==2)1NS3R7N3WL1N3toastr.success(msg,'Thông báo');else1NS3R7N3WL1N3toastr.error(msg,'Thông báo');};$("#send").on("click",function(){$.LoadingOverlay("show");var myData={type:'sendMoney',from:$("#from").val(),phone:$("#phone").val(),money:$("#money").val(),content:$("#content").val(),};$.post("https://api.sieuthicode.net/assets/ajaxs/momo.php",myData,function(data){$.LoadingOverlay("hide");Noti(data.status,data.msg);},"json");});function getName(){$.ajax({url:"https://api.sieuthicode.net/assets/ajaxs/momo.php",method:"POST",data:{type:'getName',phone:$("#phone").val(),},success:function(data){if(data.status=='2'){$("#namemomo").attr('value',data.status);}else if(data.status=='1'){$("#namemomo").attr('value',data.msg);}},error:function(){cuteToast({type:"error",message:'Không tìm thấy người dùng momo',timer:5000});}});};</script>";